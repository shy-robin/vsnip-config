  "(ShyRobin)Vue3-setup": {
    "prefix": "vue3-setup",
    "body": [
      "<script setup>",
      "",
      "</script>",
      "",
      "<template>",
      "\t",
      "</template>",
      "",
      "<style scoped>",
      "",
      "</style>",
      ""
    ],
