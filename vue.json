  "(ShyRobin)Vue3-ks": {
    "prefix": "vue3-ks",
    "body": [
      "<template>",
      "\t",
      "</template>",
      "",
      "<script lang=\"ts\">",
      "import { defineComponent } from 'vue';",
      "",
      "export default defineComponent({",
      "\tname: '${1:$TM_FILENAME_BASE}',",
      "\tsetup() {",
      "\t\treturn {",
      "\t\t}",
      "\t},",
      "});",
      "</script>",
      "",
      "<style scoped lang=\"less\">",
      "",
      "</style>",
      ""
    ],
    "description": "快速创建 Vue3-ks 模板。"
