  "(ShyRobin)Vue3-ks-setup": {
    "prefix": ["vue", "vue3-ks-setup"],
    "body": [
      "<template>",
      "\t$1",
      "</template>",
      "",
      "<script setup lang=\"ts\">",
      "$2",
      "</script>",
      "",
      "<style scoped lang=\"less\">",
      "$3",
      "</style>",
      ""
    ],
    "description": "快速创建 Vue3-ks-setup 模板。"
  },

  "Function": {
    "prefix": ["func"],
    "body": ["$3function $1($2) {", "\t$4", "}", "$5"],
    "description": "Create a normal/async function."
  },
  "ArrowFunction": {
    "prefix": ["afunc"],
    "body": ["const $1 = $3($2) => {", "\t$4", "}", "$5"],
    "description": "Create a normal/async arrow function."
  },
  "ImportStatement": {
    "prefix": ["im", "import"],
    "body": ["import { $2 } from '${1:module}';"],
    "description": "Create import statement."
  },
  "ImportDefaultStatement": {
    "prefix": ["id", "importdefault"],
    "body": ["import $2 from '${1:module}';"],
    "description": "Create import default statement."
  },
  "Tag": {
    "prefix": ["t", "tag"],
    "body": ["<$1$3>$2</$1>"],
    "description": "Create tag."
  },
  "SelfClosingTag": {
    "prefix": ["st", "stag"],
    "body": ["<$1 />"],
    "description": "Create self-closing tag."
  },
  "JsDoc": {
    "prefix": ["jsdoc"],
    "body": ["/**", " * ${1}", " */"],
    "description": "Create jsdoc."
  }
}
